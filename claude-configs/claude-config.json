    "mcpServers": {
        "news-server": {
            "command": "/Users/path/to/.local/bin/uv",
            "args": [
                "--directory",
                "/Users/path/to/stock-agent/yahoo-finance-mcp/mcp",
                "run",
                "python",
                "news_mcp.py"
            ],
            "env": {
                "SERPAPI_KEY": "SERP_API_KEY"
            }
        },
        "pead-server": {
